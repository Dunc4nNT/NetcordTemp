{
    "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
    "build": {
        "content": [
            {
                "exclude": [
                    "_site/**",
                    "scripts/**",
                    "templates/**"
                ],
                "files": [
                    "**/*.{md,yml}"
                ]
            }
        ],
        "globalMetadata": {
            "_appName": "Netcord",
            "_appTitle": "Netcord Documentation",
            "_appFooter": "Copyright Â© Dunc4nNT",
            "_appFaviconPath": "assets/images/favicon.ico",
            "_appLogoPath": "assets/images/logo_small.png",
            "_enableSearch": true,
            "_lang": "en"
        },
        "output": "_site",
        "resource": [
            {
                "files": [
                    "assets/**"
                ]
            }
        ],
        "template": [
            "default",
            "modern",
            "templates/material/material",
            "templates/netcord_template"
        ]
    },
    "metadata": [
        {
            "output": "api/Netcord/Netcord",
            "src": [
                {
